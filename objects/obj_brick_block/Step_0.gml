// Checks if you collide with the bottom of a brickif (place_meeting(x, y+1, obj_player) && place_meeting(x, y+1, obj_follower)){    bouncing = true    obj_player.vsp = 1    obj_player.jumpTime = 20}// Bouncing animationif (bouncing){    y -= 1    if (y == initial_y - 8) bouncing = false}if (!bouncing){    if (y != initial_y) y += 1}